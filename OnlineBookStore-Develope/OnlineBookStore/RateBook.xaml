<Window x:Class="OnlineBookStore.RateBook"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnlineBookStore"
        mc:Ignorable="d"
        Title="RateBook" WindowStyle="None" WindowStartupLocation="CenterOwner"  Background="Bisque"  ResizeMode="NoResize" AllowsTransparency="False" Height="150" Width="300">
    <Grid>
        <ComboBox Name="combo" VerticalAlignment="Top" Margin="50,42,50,0" Width="200" Height="20">
            <ComboBoxItem Content="1"/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="5"/>
            <ComboBoxItem Content="6"/>
            <ComboBoxItem Content="7"/>
            <ComboBoxItem Content="8"/>
            <ComboBoxItem Content="9"/>
            <ComboBoxItem Content="10"/>
        </ComboBox>
        <Button Content="Cancel" Background="OrangeRed" Name="cancel" Click="cancel_Click" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="88,90,0,0"/>
        <Button Content="Ok" Background="YellowGreen" Name="ok" Click="ok_Click" Width="50" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,90,80,0"/>
    </Grid>
</Window>
